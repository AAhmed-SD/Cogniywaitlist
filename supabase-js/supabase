import { createClient } from '@supabase/supabase-js';

// Supabase credentials
const supabaseUrl = 'https://koidkotynqdkidkvubcd.supabase.co'; // Replace with your Supabase URL
const supabaseKey = 'your-anon-or-service-role-key'; // Replace with your Supabase API key

// Create a Supabase client
const supabase = createClient(supabaseUrl, supabaseKey);

// Example: Fetch data from a table
const fetchData = async () => {
    const { data, error } = await supabase
        .from('your_table_name') // Replace 'your_table_name' with the name of your table
        .select('*');

    if (error) {
        console.error('Error fetching data:', error);
    } else {
        console.log('Data:', data);
    }
};

fetchData();